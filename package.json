{
  "name": "kontrol-mean",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": " concurrently -r \"npm run start:server\" \"npm run start:client\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:client": "ng serve --poll=2000",
    "start:server": "NODE_ENV=development DEBUG=kontrol-MEAN* nodemon --exec babel-node server/index.js ",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build --configuration=production",
    "build:server": "babel server --out-dir dist/server",
    "start:prod": "NODE_ENV=production DEBUG=kontrol-MEAN* nodemon --exec babel-node server/index.js ",
    "start:prod2": "NODE_ENV=production node server.js"
  },
  "private": true,
